  "version": "1.0.1",
  "license": "MIT",
  "author": "Steven Vachon <contact@svachon.com> (https://svachon.com)",
  "repository": "github:stevenvachon/parse-string-boolean",
  "dependencies": {
    "is-string": "^1.0.4"
  },
  "devDependencies": {
    "babelify": "^8.0.0",
